<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AnalisisTexto">

    <Button
        android:id="@+id/u3a6idBtFinAnalisis"
        android:layout_width="186dp"
        android:layout_height="72dp"
        android:layout_marginTop="72dp"
        android:text="ANALIZAR"
        app:layout_constraintEnd_toEndOf="@+id/u3a6idEtOutput"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/u3a6idEtOutput"
        app:layout_constraintTop_toBottomOf="@+id/u3a6idEtOutput" />

    <EditText
        android:id="@+id/u3a6idEtOutput"
        android:layout_width="240dp"
        android:layout_height="83dp"
        android:layout_marginTop="84dp"
        android:ems="10"
        android:inputType="text"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/u3a6idTvPodium"
        android:layout_width="135dp"
        android:layout_height="89dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="@+id/u3a6idBtFinAnalisis"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/u3a6idBtFinAnalisis"
        app:layout_constraintTop_toBottomOf="@+id/u3a6idBtFinAnalisis" />

</androidx.constraintlayout.widget.ConstraintLayout>